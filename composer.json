{
    "name": "drparham/stubfeature",
    "description": "Stub out classes/directory structure for adding features.",
    "keywords": [
        "drparham",
        "stubfeature"
    ],
    "homepage": "https://github.com/drparham/stubfeature",
    "license": "MIT",
    "authors": [
        {
            "name": "Dustin Parham",
            "email": "dr.parham@gmail.com",
            "homepage": "",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "~5.0",
        "pta/formbuilder": "0.2.*",
        "rinvex/repository": "2.0.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "psr-4": {
            "Drparham\\StubFeature\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drparham\\StubFeature\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
